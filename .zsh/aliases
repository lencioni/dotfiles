alias :q="exit"
alias :e="vim"

alias be="bundle exec"

alias g="git"
alias gb="git branch"
alias gbd="git branch -d"
alias gbD="git branch -D"
alias gc="git commit"
alias gco="git checkout"
alias gr="git refresh"
alias gp="git add --patch"
alias ga="git commit --amend"
alias grc="git rebase --continue"
alias gri="git rebase --interactive"

alias vim="nvim"
